{
    "version": 2,
    "builds": [
        { "src": "packages/client/package.json", "use": "@vercel/node" },
        { "src": "packages/server/package.json", "use": "@vercel/node" }
    ],
    "routes": [
        { "src": "/api/(.*)", "dest": "packages/server/$1" },
        { "src": "/(.*)", "dest": "packages/client/$1" }
    ]
}
